[flake8]
max-line-length = 119
ignore = E121,E123,E126,E133,E226,E241,E242,E704,W503,W504,W505,E127,E203,E266,E402,W605,W391,E701,E731
per-file-ignores = __init__.py:F401
exclude =
    .git
    __pycache__
    setup.py
    build
    dist
    releases
    .venv
    .tox
    .mypy_cache
    .pytest_cache
    .vscode
    .github
    poetry/utils/_compat.py
    poetry/utils/env_scripts/tags.py
    tests/fixtures/
    tests/repositories/fixtures/
    tests/utils/fixtures/

